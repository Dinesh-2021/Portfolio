            jQuery.ajax({
                url: "https://api.twitch.tv/kraken/streams/medrybw",
                jsonp: "callback",
                dataType: "jsonp",
                data: {
                    q: "select 	title,abstract,url from search.news where query=\"\"",
                    format: "json"
                },
                success: function (response) {
                    $('.all').append();
                    if (response.status === 422){
                        $('.error').append('<p><i>Sadly, medrybw has retired from twitch.tv. That channel is now gone.</i></p>');
                    }
                    if (response.stream == null) {
                        $('.offline').append('<p><a href = "http://www.js.twitch.tv/medrybw">medrybw</a></p>');
                    } else {
                        $('.streamName').append('<p><a href = "http://www.js.twitch.tv/medrybw">medrybw</a></p>');
                        $('.streamInfo').append(response.stream.channel.status.toString());
                    }
                }
            });
