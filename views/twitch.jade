            jQuery.ajax({
                url: "https://api.twitch.tv/kraken/streams/medrybw",
                jsonp: "callback",
                dataType: "jsonp",
                data: {
                    q: "select 	title,abstract,url from search.news where query=\"\"",
                    format: "json"
                },
                success: function (response) {
                    console.log(response);
                    $('.all').append();
                    if (response.stream == null) {
                        $('.offline').append('<p><a href = "http://www.twitch.tv/medrybw">medrybw</a></p>');
                    } else {
                        $('.online').append('<p><a href = "http://www.twitch.tv/medrybw">medrybw</a></p>');
                        $('.online').append(response.stream.channel.status.toString());
                    }
                }
            });
